{
	"name": "maple-sdk",
	"version": "0.0.7",
	"keywords": [
		"analytics",
		"web",
		"react"
	],
	"author": "Makisuo",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/MapleAnalytics/maple-sdk",
		"directory": "packages/maple-sdk"
	},
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs"
		},
		"./react": {
			"require": "./dist/react.js",
			"import": "./dist/react.mjs"
		},
		"./tracker": {
			"require": "./dist/tracker.js",
			"import": "./dist/tracker.mjs"
		}
	},
	"files": [
		"dist",
		"package.json",
		"LICENSE",
		"README.md"
	],
	"scripts": {
		"start": "node dist/bin.js",
		"build": "tsup",
		"dev": "tsup --watch",
		"lint": "eslint . --ext .ts",
		"test": "vitest run",
		"test:watch": "vitest",
		"typecheck": "tsc --noEmit"
	},
	"optionalDependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@types/react": "18.0.38",
		"eslint": "^8.39.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"tsup": "^6.7.0",
		"typescript": "^5.0.4"
	}
}
