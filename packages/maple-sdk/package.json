{
	"name": "maple-sdk",
	"version": "0.4.0",
	"type": "module",
	"keywords": [
		"analytics",
		"web",
		"react"
	],
	"private": "false",
	"author": "Makisuo",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/MapleAnalytics/maple-sdk",
		"directory": "packages/maple-sdk"
	},
	"exports": {
		".": {
			"browser": "./dist/index.js",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./react": {
			"browser": "./dist/react/index.js",
			"import": "./dist/react/index.js",
			"require": "./dist/react/index.cjs"
		},
		"./tracker": {
			"browser": "./dist/tracker.js",
			"require": "./dist/tracker.cjs",
			"import": "./dist/tracker.mjs",
			"types": "./dist/tracker.d.ts"
		},
		"./package.json": "./package.json"
	},
	"typesVersions": {
		"*": {
			"*": [
				"dist/index.d.ts"
			],
			"react": [
				"dist/react/index.d.ts"
			],
			"tracker": [
				"dist/tracker.d.ts"
			]
		}
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist",
		"package.json",
		"LICENSE",
		"README.md"
	],
	"scripts": {
		"start": "node dist/bin.js",
		"build": "tsup",
		"dev": "tsup --watch",
		"lint": "eslint . --ext .ts",
		"test": "vitest run",
		"test:watch": "vitest",
		"typecheck": "tsc --noEmit"
	},
	"devDependencies": {
		"@types/react": "18.2.8",
		"eslint": "^8.42.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"tsup": "^6.7.0",
		"typescript": "^5.1.3"
	}
}