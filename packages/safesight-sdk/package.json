{
  "name": "safesight",
  "version": "0.6.9",
  "type": "module",
  "keywords": [
    "analytics",
    "web",
    "react"
  ],
  "private": false,
  "author": "Makisuo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MapleAnalytics/maple-sdk",
    "directory": "packages/safesight-sdk"
  },
  "scripts": {
    "start": "node dist/bin.js",
    "build": "bunchee",
    "dev": "tsup --watch",
    "lint": "eslint . --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "changelog:generate": "changelogen --bump",
    "changelog:release": "pnpm run build && changelogen --release --publish"
  },
  "files": [
    "dist",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "peerDependencies": {
    "react": ">18.0.0",
    "react-dom": ">18.0.0",
    "@builder.io/qwik": ">1.0.0"
  },
  "peerDependenciesMeta": {
    "@builder.io/qwik": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@builder.io/qwik": "^1.4.2",
    "@types/react": "18.2.48",
    "bunchee": "^4.4.2",
    "changelogen": "^0.5.5",
    "typescript": "^5.3.3"
  },
  "main": "./dist/es/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/es/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/es/index.d.ts",
        "default": "./dist/es/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./react": {
      "import": {
        "types": "./dist/es/react.d.ts",
        "default": "./dist/es/react.js"
      },
      "require": {
        "types": "./dist/cjs/react.d.cts",
        "default": "./dist/cjs/react.cjs"
      }
    },
    "./qwik": {
      "import": {
        "types": "./dist/es/qwik.d.ts",
        "default": "./dist/es/qwik.js"
      },
      "require": {
        "types": "./dist/cjs/qwik.d.cts",
        "default": "./dist/cjs/qwik.cjs"
      }
    },
    "./tracker": {
      "import": {
        "types": "./dist/es/tracker.d.ts",
        "default": "./dist/es/tracker.js"
      },
      "require": {
        "types": "./dist/cjs/tracker.d.cts",
        "default": "./dist/cjs/tracker.cjs"
      }
    }
  }
}