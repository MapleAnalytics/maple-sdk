{
  "name": "safesight",
  "version": "0.6.1",
  "type": "module",
  "keywords": [
    "analytics",
    "web",
    "react"
  ],
  "private": false,
  "author": "Makisuo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MapleAnalytics/maple-sdk",
    "directory": "packages/safesight-sdk"
  },
  "scripts": {
    "start": "node dist/bin.js",
    "build": "bunchee",
    "dev": "tsup --watch",
    "lint": "eslint . --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "changelog:generate": "changelogen --bump",
    "changelog:release": "pnpm run build && changelogen --release --publish"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/es/index.d.ts",
        "default": "./dist/es/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./react": {
      "import": {
        "types": "./dist/es/react.d.ts",
        "default": "./dist/es/react.js"
      },
      "require": {
        "types": "./dist/cjs/react.d.cts",
        "default": "./dist/cjs/react.cjs"
      }
    },
    "./tracker": {
      "import": {
        "types": "./dist/es/tracker.d.ts",
        "default": "./dist/es/tracker.js"
      },
      "require": {
        "types": "./dist/cjs/tracker.d.cts",
        "default": "./dist/cjs/tracker.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ],
      "react": [
        "dist/react/index.d.ts"
      ],
      "tracker": [
        "dist/tracker.d.ts"
      ]
    }
  },
  "main": "./dist/es/index.js",
  "types": "./dist/es/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@types/react": "18.2.8",
    "bunchee": "^4.4.2",
    "changelogen": "^0.5.5",
    "eslint": "^8.42.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3"
  },
  "module": "./dist/es/index.js"
}